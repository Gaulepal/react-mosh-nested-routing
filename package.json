{
  "name": "react-typescript-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --quiet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
